{
  "name": "elm-doist",
  "description": "elm-doist",
  "version": "1.0.0",
  "main": "index.js",
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "scripts": {
    "wds:start": "nodemon -w webpack.config.js -w package-lock.json -w elm.json -x webpack-dev-server -- -d --open",
    "wp:build": "env NODE_ENV=production webpack -p",
    "pl:start": "npx parcel src/index.html --open",
    "pl:build": "npx parcel build src/index.html",
    "start": "npm run pl:start",
    "build": "npm run pl:build",
    "deploy": "npx firebase deploy --only hosting:elmdoist",
    "stage": "npm run build && npm run deploy"
  },
  "private": true,
  "keywords": [],
  "author": "Jigar Gosar <jigar.gosar@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "faker": "^4.1.0",
    "firebase": "^6.3.4",
    "fp-ts": "^2.0.3",
    "nanoid": "^2.0.3",
    "ow": "^0.13.2",
    "ramda": "^0.26.1",
    "tachyons": "^4.11.1"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "elm-hot": "^1.1.2",
    "@types/faker": "^4.1.5",
    "@types/nanoid": "^2.0.0",
    "@types/ramda": "^0.26.18",
    "@types/webpack-env": "^1.14.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.1.0",
    "elm-webpack-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.1",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.6.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
